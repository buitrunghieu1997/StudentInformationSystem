                                             </children>
                                          </HBox>
                                          <HBox styleClass="vbox">
                                             <children>
                                                <JFXComboBox fx:id="semesterCombo" focusColor="#485460" minHeight="-Infinity" minWidth="-Infinity" prefHeight="39.0" prefWidth="175.0" promptText="Semester" unFocusColor="#48535e4f" />
