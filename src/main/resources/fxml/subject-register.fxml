                                       </children>
                                    </HBox>
                                    <HBox styleClass="vbox">
                                       <children>
                                          <TextField fx:id="subjectIdTxt" minHeight="-Infinity" minWidth="-Infinity" prefWidth="175.0" promptText="Enter Subject ID here" styleClass="txt-field-border" HBox.hgrow="ALWAYS">
