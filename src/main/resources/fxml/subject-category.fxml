<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.lang.String?>
<VBox xmlns:fx="http://javafx.com/fxml/1" id="back-box" prefHeight="604.0" prefWidth="930.0"
      stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171"
      fx:controller="controller.SubjectCategoryController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <HBox alignment="CENTER_LEFT" styleClass="vbox">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="component-inside" text="Subject Category"/>
            </children>
        </HBox>
        <VBox>
            <children>
                <HBox alignment="CENTER" styleClass="vbox">
                    <children>
                        <VBox alignment="CENTER" styleClass="vbox" HBox.hgrow="ALWAYS">
                            <children>
                                <HBox>
                                    <children>
                                        <VBox>
                                            <children>
                                                <VBox styleClass="vbox">
                                                    <children>
                                                        <HBox styleClass="vbox">
                                                            <children>
                                                                <Text strokeType="OUTSIDE" strokeWidth="0.0"
                                                                      styleClass="acc-info" text="Search By Department"
                                                                      HBox.hgrow="ALWAYS"/>
                                                            </children>
                                                        </HBox>
                                                        <HBox styleClass="vbox">
                                                            <children>
                                                                <JFXComboBox fx:id="departmentCombo"
                                                                             focusColor="#485460" minHeight="-Infinity"
                                                                             prefHeight="39.0" prefWidth="240.0"
                                                                             promptText="Department of HUST"
                                                                             unFocusColor="#48535e4f"/>
                                                            </children>
                                                        </HBox>
                                                    </children>
                                                    <padding>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </padding>
                                                </VBox>
                                            </children>
                                        </VBox>
                                        <VBox styleClass="vbox" HBox.hgrow="ALWAYS">
                                            <children>
                                                <HBox styleClass="vbox">
                                                    <children>
                                                        <Text strokeType="OUTSIDE" strokeWidth="0.0"
                                                              styleClass="acc-info" text="Search By Subject ID"
                                                              HBox.hgrow="ALWAYS"/>
                                                    </children>
                                                </HBox>
                                                <HBox styleClass="vbox">
                                                    <children>
                                                        <TextField fx:id="subjectIdTxt" prefWidth="165.0"
                                                                   promptText="Subject ID" styleClass="txt-field-border"
                                                                   HBox.hgrow="ALWAYS">
                                                            <padding>
                                                                <Insets bottom="10.0" left="10.0" right="10.0"
                                                                        top="10.0"/>
                                                            </padding>
                                                        </TextField>
                                                    </children>
                                                </HBox>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </VBox>
                                        <VBox layoutX="10.0" layoutY="10.0" styleClass="vbox" HBox.hgrow="ALWAYS">
                                            <children>
                                                <HBox styleClass="vbox">
                                                    <children>
                                                        <Text strokeType="OUTSIDE" strokeWidth="0.0"
                                                              styleClass="acc-info" text="Search By Subject Name"
                                                              HBox.hgrow="ALWAYS"/>
                                                    </children>
                                                </HBox>
                                                <HBox styleClass="vbox">
                                                    <children>
                                                        <TextField fx:id="subjectNameTxt" promptText="Subject Name"
                                                                   styleClass="txt-field-border" HBox.hgrow="ALWAYS">
                                                            <padding>
                                                                <Insets bottom="10.0" left="10.0" right="10.0"
                                                                        top="10.0"/>
                                                            </padding>
                                                        </TextField>
                                                    </children>
                                                </HBox>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </VBox>
                                    </children>
                                </HBox>
                            </children>
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </VBox>
                        <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" styleClass="vbox">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                            <children>
                                <HBox styleClass="vbox">
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="acc-info"
                                              HBox.hgrow="ALWAYS"/>
                                    </children>
                                </HBox>
                                <Button fx:id="search" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                                        prefHeight="39.0" prefWidth="100.0" text="Search">
                                    <styleClass>
                                        <String fx:value="button-text"/>
                                        <String fx:value="cursor-hand"/>
                                    </styleClass>
                                </Button>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER" styleClass="vbox">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                            <children>
                                <HBox styleClass="vbox">
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="acc-info"
                                              HBox.hgrow="ALWAYS"/>
                                    </children>
                                </HBox>
                                <Button fx:id="back" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                                        prefHeight="39.0" prefWidth="100.0" text="Back">
                                    <styleClass>
                                        <String fx:value="button-text"/>
                                        <String fx:value="cursor-hand"/>
                                    </styleClass>
                                </Button>
                            </children>
                        </VBox>
                    </children>
                </HBox>
            </children>
        </VBox>
        <VBox styleClass="vbox" VBox.vgrow="ALWAYS">
            <children>
                <ScrollPane hbarPolicy="NEVER" styleClass="vbox" VBox.vgrow="ALWAYS">
                    <content>
                        <VBox alignment="CENTER" prefHeight="420.0" prefWidth="900.0" styleClass="vbox">
                            <children>
                                <TableView fx:id="tableSC" stylesheets="@../css/style.css" tableMenuButtonVisible="true"
                                           VBox.vgrow="ALWAYS">
                                    <placeholder>
                                        <Label text="There is no subject in this category."/>
                                    </placeholder>
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </TableView>
                            </children>
                        </VBox>
                    </content>
                </ScrollPane>
            </children>
        </VBox>
    </children>
</VBox>
